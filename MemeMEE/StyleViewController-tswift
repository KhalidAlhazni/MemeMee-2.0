import Foundation
import UIKit
extension StyleViewController: UITextFieldDelegate{
    //Keyboard
    //keyboardshow
    
    func setupTextField(_ textField: UITextField) {
        textField.defaultTextAttributes = styleTextAttributes
        textField.textAlignment = .center
    }
    
}
